---
import BaseLayout from "./BaseLayout.astro";

const page = Astro.props;
---

<BaseLayout>
  <main class="md:flex md:justify-center w-full">
    <article class="prose prose-lg max-w-[750px] prose-img:mx-auto">
      <h1>{page.data?.title}</h1>
      <slot />
    </article>
  </main>
</BaseLayout>
